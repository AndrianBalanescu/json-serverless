{
  "name": "json-server-less-lambda",
  "version": "1.0.0",
  "description": "a lambda solution for json-server",
  "main": "src/server.js",
  "scripts": {
    "start:dev": "webpack-dev-server",
    "test": "npm run build && NODE_ENV=local jest",
    "debug": "NODE_ENV=local nodemon --exec babel-node src/handler.js --inspect",
    "start": "NODE_ENV=local nodemon --exec babel-node src/handler.js",
    "dev": "NODE_ENV=diagnostic  nodemon --exec babel-node src/handler.js --inspect",
    "diagnostic": "sls offline",
    "build": "babel src -d dist -D",
    "deploy": "serverless deploy --stage=dev",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pharindoko/ngx-chartboard-backend.git"
  },
