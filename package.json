    "copy-webpack-plugin": "^5.0.4",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "serverless-offline": "^5.8.0",
    "serverless-webpack": "^5.3.1",
    "webpack": "^4.36.1",
    "webpack-node-externals": "^1.7.2"
  },
