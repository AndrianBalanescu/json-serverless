  },
  "homepage": "https://github.com/pharindoko/ngx-chartboard-backend#readme",
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "@babel/runtime": "^7.5.5",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.0.0",
    "json-server": "^0.15.0",
    "lowdb": "^1.0.0",
    "lowdb-adapter-aws-s3": "^1.1.2",
    "pino": "^5.10.6",
    "pino-pretty": "^3.2.0",
    "serverless-http": "^2.0.0",
    "snyk": "^1.193.0",
    "supertest": "^4.0.2"
